    chown system camera /dev/T8EV3AF
    chown system camera /dev/CAM_CAL_DRV


    #/dev/RT_Monitor
    chmod 0600 /dev/RT_Monitor
    chown system system /dev/RT_Monitor



    #MATV
    chmod 0660 /dev/MATV
    chown system media /dev/MATV

    # otg_test
